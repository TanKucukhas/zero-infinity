		"react-hook-form": "^7.65.0",
		"react-perfect-scrollbar": "^1.5.8",
		"react-use": "^17.5.0",
		"server-only": "^0.0.1",
		"tailwind-merge": "^2.5.4",
		"tailwindcss": "^3.4.18",
		"tailwindcss-animate": "^1.0.7",
		"tailwindcss-logical": "^3.0.1",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
